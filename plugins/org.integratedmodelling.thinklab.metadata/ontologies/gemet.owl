<skos:narrower rdf:resource="#Concept14884"/>
<skos:narrower rdf:resource="#Concept3907"/>
<skos:narrower rdf:resource="#Concept4814"/>
<skos:narrower rdf:resource="#Concept5428"/>
<skos:narrower rdf:resource="#Concept8633"/>
<skos:narrower rdf:resource="#Concept14940"/>
<skos:narrower rdf:resource="#Concept2746"/>
<skos:narrower rdf:resource="#Concept3599"/>
<skos:narrower rdf:resource="#Concept8350"/>
<skos:narrower rdf:resource="#Concept9244"/>
<skos:broader rdf:resource="#Concept7621"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept2167">
<rdfs:label xml:lang="en">dialysis</rdfs:label>
<rdfs:comment xml:lang="en">A process of selective diffusion through a membrane; usually used to separate low-molecular-weight solutes which diffuse through the membrane from the colloidal and high-molecular-weight solutes which do not.
(Source: MGH)</rdfs:comment>
