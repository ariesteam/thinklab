<skos:broader rdf:resource="#Concept6663"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept6953">
<rdfs:label xml:lang="en">raising a site</rdfs:label>
<rdfs:comment xml:lang="en">The building up of land by the deposition of allochthonous material, such as rocks, gravel, etc.
(Source: RRDA)</rdfs:comment>
<skos:broader rdf:resource="#Concept4257"/>
