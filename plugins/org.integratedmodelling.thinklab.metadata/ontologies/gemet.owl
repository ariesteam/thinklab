<skos:narrower rdf:resource="#Concept6750"/>
<skos:broader rdf:resource="#Concept10064"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept9259">
<rdfs:label xml:lang="en">wave energy</rdfs:label>
<rdfs:comment xml:lang="en">Power extracted from the motion of sea waves at the coast.
(Source: CED)</rdfs:comment>
<skos:narrower rdf:resource="#Concept8475"/>
<skos:broader rdf:resource="#Concept5668"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept9184">
<rdfs:label xml:lang="en">water hardness</rdfs:label>
<rdfs:comment xml:lang="en">The amount of calcium and magnesium salts dissolved in water.
(Source: MGHa)</rdfs:comment>
<skos:broader rdf:resource="#Concept1673"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept8893">
<rdfs:label xml:lang="en">vanadium</rdfs:label>
<rdfs:comment xml:lang="en">A silvery-white, ductile metal resistant to corrosion; used in alloy steels and as an x-ray target.
(Source: MGH)</rdfs:comment>
<skos:broader rdf:resource="#Concept8629"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept6128">
<rdfs:label xml:lang="en">peroxyacetylnitrate</rdfs:label>
<rdfs:comment xml:lang="en">A pollutant created by the action of sunlight on hydrocarbons and nitrogen oxides in the air. An ingredient of smog.
(Source: LANDY)</rdfs:comment>
<skos:broader rdf:resource="#Concept5604"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept13242">
<rdfs:label xml:lang="en">commercial transaction</rdfs:label>
<rdfs:comment xml:lang="en">The conduct or carrying on of trade, business or a financial matter to a conclusion or settlement.
(Source: RHW)</rdfs:comment>
<skos:narrower rdf:resource="#Concept183"/>
<skos:narrower rdf:resource="#Concept10260"/>
<skos:narrower rdf:resource="#Concept6835"/>
<skos:narrower rdf:resource="#Concept8569"/>
