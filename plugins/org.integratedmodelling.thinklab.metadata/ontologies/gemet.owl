<skos:broader rdf:resource="#Concept170"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept3032">
<rdfs:label xml:lang="en">excavation site</rdfs:label>
<rdfs:comment xml:lang="en">The location chosen for an excavation, meaning the act or process of removing soil and/or rock materials by digging, blasting, breaking, loading either at the surface or underground.
(Source: BJGEOa)</rdfs:comment>
<skos:narrower rdf:resource="#Concept9367"/>
<skos:narrower rdf:resource="#Concept3028"/>
<skos:broader rdf:resource="#Concept5286"/>
