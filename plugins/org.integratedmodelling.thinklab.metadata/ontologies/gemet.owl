<skos:Concept rdf:about="#Concept6600">
<rdfs:label xml:lang="en">preventive health measure</rdfs:label>
<skos:broader rdf:resource="#Concept3877"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept8876">
<rdfs:label xml:lang="en">local resource utilisation</rdfs:label>
