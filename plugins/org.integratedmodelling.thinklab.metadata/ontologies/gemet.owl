<rdfs:label xml:lang="en">mountain protection</rdfs:label>
<skos:broader rdf:resource="#Concept10032"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept7178">
<rdfs:label xml:lang="en">respiratory air</rdfs:label>
<rdfs:comment xml:lang="en">Air volumes inspired and expired through the lungs.
(Source: DELFIN)</rdfs:comment>
<skos:broader rdf:resource="#Concept7176"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept9358">
