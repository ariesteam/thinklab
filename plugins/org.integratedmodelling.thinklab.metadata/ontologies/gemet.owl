<skos:narrower rdf:resource="#Concept2469"/>
<skos:broader rdf:resource="#Concept7781"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept7363">
<rdfs:label xml:lang="en">safety analysis</rdfs:label>
