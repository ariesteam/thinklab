<skos:broader rdf:resource="#Concept6225"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept2947">
<rdfs:label xml:lang="en">epidemic</rdfs:label>
<rdfs:comment xml:lang="en">A sudden increase in the incidence rate of a disease to a value above normal, affecting large numbers of people and spread over a wide area.
(Source: MGH)</rdfs:comment>
<skos:narrower rdf:resource="#Concept8906"/>
<skos:broader rdf:resource="#Concept4046"/>
</skos:Concept>
