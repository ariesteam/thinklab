<skos:narrower rdf:resource="#Concept5667"/>
<skos:broader rdf:resource="#Group1062"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept4728">
<rdfs:label xml:lang="en">lead level in blood</rdfs:label>
<rdfs:comment xml:lang="en">A measure of the amount of lead or lead salts absorbed by the body as a possible sign of acute or chronic lead poisoning, which can affect the nervous, digestive or muscular systems.
(Source: SMD / MED)</rdfs:comment>
<skos:broader rdf:resource="#Concept6359"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept3130">
<rdfs:label xml:lang="en">farm</rdfs:label>
<rdfs:comment xml:lang="en">Any tract of land or building used for agricultural purposes, such as for raising crops and livestock.
(Source: RHW)</rdfs:comment>
<skos:narrower rdf:resource="#Concept3123"/>
<skos:narrower rdf:resource="#Concept13192"/>
<skos:broader rdf:resource="#Concept13136"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept10002">
<rdfs:label xml:lang="en">accounting</rdfs:label>
<rdfs:comment xml:lang="en">Method of recording all the transactions affecting the financial condition of a business or organization.</rdfs:comment>
<skos:narrower rdf:resource="#Concept709"/>
<skos:narrower rdf:resource="#Concept961"/>
<skos:narrower rdf:resource="#Concept3049"/>
<skos:narrower rdf:resource="#Concept2767"/>
<skos:broader rdf:resource="#Concept1084"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept3171">
<rdfs:label xml:lang="en">fertiliser law</rdfs:label>
<skos:broader rdf:resource="#Concept206"/>
</skos:Concept>
<skos:Concept rdf:about="#Concept4730">
<rdfs:label xml:lang="en">leaf</rdfs:label>
<rdfs:comment xml:lang="en">The main organ of photosynthesis and transpiration in higher plants, usually consisting of a flat green blade attached to the stem directly or by a stalk.
(Source: CED)</rdfs:comment>
<skos:broader rdf:resource="#Concept6300"/>
</skos:Concept>
