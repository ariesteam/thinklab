<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns="http://www.integratedmodelling.org/ks/metadata/metadata.owl#"
  xml:base="http://www.integratedmodelling.org/ks/metadata/metadata.owl">
  <owl:Ontology rdf:about=""/>
  <owl:DatatypeProperty rdf:ID="hasWGS84Latitude">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasURL">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasSourceCitation">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The text of a citation that documents the associated instance.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasWGS84Longitude">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasAssociatedEventDate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasPriority">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasAssociatedEventDescriptor">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasDownloadLink">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasImageUrl">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The URL of an image that documents the associated instance.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </owl:DatatypeProperty>
  <owl:AnnotationProperty rdf:ID="belongsToDataset">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Used to identify annotations that are part of the same dataset. These are treated differently in the modeling system, which is allowed to fill holes from multiple observations that match the same datasets even if only one contingent model has been specified.</rdfs:comment>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:ID="hasISIClassification">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:AnnotationProperty>
</rdf:RDF>

