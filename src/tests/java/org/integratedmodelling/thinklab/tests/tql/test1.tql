namespace org.integratedmodelling.test1;

defmodel rainfall of Rainfall
	as measure habitat:Rainfall in m;

/**
 * this uses a datasource so it is stored with its computed extents to resolve 
 * observables later. 
 */	
model wcs(service="http://ecoinformatics.uvm.edu/geoserver/wcs", id="global:annual_precip_global")
     as measure habitat:Rainfall in mm
     with metadata {
              dc:comment ""
              dc:originator ""
              dc:rights "cc-attr-nomod"
       };
	
	
defcontext puget as
     grid(resolution="1 km", 
	  shape="EPSG:4326 POLYGON((-124.7625 24.5210, -66.9326  24.5210, -66.9326 49.3845, -124.7625 49.3845, -124.7625 24.5210))");
	